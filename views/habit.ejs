
<link rel="stylesheet" href="/css/habit.css">

<!-- Habit Name -->
<div>
    <h1 class="heading-name">
        <%=habit.habit_name%>
    </h1>
</div>

<!-- 
Date filer -->
<div class="date-container ">
    <div>
        From :
        <input type="date" name="start-date" id="start-date" class="date-picker" required>
    </div>
    <div>
        to :
        <input type="date" name="end-date" id="end-date" class="date-picker" required>
    </div>
    <button id="filter"><i class="fas fa-search"></i></button>
</div>


<!-- colors -->
<div class="key">
    <span>Status:</span>
    <span style="color: gray;">None</span>
    <span style="color: green;">Done</span>
    <span style="color: red;">Not Done </span>
</div>


<!-- // habit list  -->
<div class="habit-container">
    <ul id="item-list">
        <!-- <li class="list-item">
            <div class="date-div"></div>
            <div class="status"></div>
        </li> -->
    </ul>
</div>




<!-- scripts -->
<script src="/js/moment.js"></script>
<script src="/js/habit.js"></script>